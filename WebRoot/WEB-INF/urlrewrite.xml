<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/article/([0-9]+).html$</from>
		<to type="forward">/article/newscontentArticle.do?catId=$1</to>
	</rule>
	<rule>
		<from>^/gl/([0-9]+).html$</from>
		<to >/article/newscontentArticle.do?catId=$1&amp;type=hujin</to>
	</rule>
	<rule>
		<from>^/gonglue/$</from>
		<to >/html/financialStrategySingle.do</to>
	</rule>
	<rule>
		<from>^/gonglue/([0-9]+)/$</from>
		<to >/html/mutualFinancialSingle.do?id=$1</to>
	</rule>
    <rule>
        <from>^/gonglue/fl/([0-9]+)/$</from>
        <to >/html/thirdFinancialSingle.do?id=$1</to>
    </rule>
</urlrewrite>