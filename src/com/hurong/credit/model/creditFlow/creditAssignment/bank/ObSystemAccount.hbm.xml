<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.hurong.credit.model.creditFlow.creditAssignment.bank.ObSystemAccount"
        table="ob_system_account"
        select-before-update="true"
        dynamic-update="true"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="id"
            column="id"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <version name="version" column="version" type="java.lang.Integer" ></version>
         <property
            name="accountName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountName"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="accountNumber"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="accountNumber"
            length="100"
            not-null="false"
            unique="false"
        />
        <property
            name="investmentPersonId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="investmentPersonId"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="totalMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="totalMoney"
            precision="40"
            scale="4"
            not-null="false"
            unique="false"
        />
        
         <property
            name="investPersionType"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="investPersionType"
            length="1"
            not-null="false"
            unique="false"
        />
        <property
            name="accountStatus"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="accountStatus"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="investPersonName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="investPersonName"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="companyId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="companyId"
            length="19"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
