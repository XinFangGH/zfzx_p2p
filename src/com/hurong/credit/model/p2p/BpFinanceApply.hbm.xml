<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="com.hurong.credit.model.p2p.BpFinanceApply"
        table="bp_finance_apply"
        select-before-update="false"
        dynamic-update="false"
        dynamic-insert="false"
        optimistic-lock="version"
    >
		<id
            name="loanId"
            column="loanId"
            type="java.lang.Long"
            length="19"
        >
            <generator class="native"/>
        </id>
         <property
            name="productId"
            type="java.lang.Long"
            update="true"
            insert="true"
            access="property"
            column="productId"
            length="19"
            not-null="false"
            unique="false"
        />
        
         <property
            name="productName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="productName"
            length="100"
            not-null="false"
            unique="false"
        />
        
         <property
            name="businessName"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="businessName"
            length="100"
            not-null="false"
            unique="false"
        />
         <property
            name="type"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="type"
            length="2"
            not-null="false"
            unique="false"
        />
        <property
            name="linkPersion"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="linkPersion"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="phone"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="phone"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="loanMoney"
            type="java.math.BigDecimal"
            update="true"
            insert="true"
            access="property"
            column="loanMoney"
            precision="20"
            scale="4"
            not-null="false"
            unique="false"
        />
        <property
            name="isOnline"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="isOnline"
            length="5"
            not-null="false"
            unique="false"
        />
        <property
            name="loanTimeLen"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="loanTimeLen"
            length="50"
            not-null="false"
            unique="false"
        />
        <property
            name="area"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="area"
            length="20"
            not-null="false"
            unique="false"
        />
        <property
            name="remark"
            type="java.lang.String"
            update="true"
            insert="true"
            access="property"
            column="remark"
            length="255"
            not-null="false"
            unique="false"
        />
        <property
            name="createTime"
            type="java.util.Date"
            update="true"
            insert="true"
            access="property"
            column="createTime"
            length="19"
            not-null="false"
            unique="false"
        />
        <property
            name="state"
            type="java.lang.Short"
            update="true"
            insert="true"
            access="property"
            column="state"
            length="5"
            not-null="false"
            unique="false"
        />
     </class>
</hibernate-mapping>
